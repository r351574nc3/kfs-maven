            <ref bean="travelPerDiemFiscalYearMaker" />
            <ref bean="fiscalYearFunctionControlFiscalYearMaker" />
          </list>	
        </property>
        <property name="batchFileDirectories">
          <list>
            <value>${reports.directory}/fp</value>
            <value>${staging.directory}/fp</value>
          </list>
        </property>

	</bean>

