            </list>
        </property>
        <property name="fiscalYearMakers">
          <list>
            <ref bean="systemOptionsFiscalYearMaker" />
            <ref bean="universityDateFiscalYearMaker" />
          </list>	
        </property>
        <property name="batchFileDirectories">
          <list>
            <value>${reports.directory}/sys</value>
          </list>
        </property>

