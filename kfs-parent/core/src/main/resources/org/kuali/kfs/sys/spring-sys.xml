		</property>
	</bean>
	
	<bean id="autoDisapproveErrorReportWriterService" parent="reportWriterService">
		<property name="filePath" value="${reports.directory}/sys"/>
		<property name="fileNamePrefix" value="autoDisapproveErrors"/>
