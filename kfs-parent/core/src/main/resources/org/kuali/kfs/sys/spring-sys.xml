            </list>
        </property>
        <property name="jobNames">
            <list>
				<value>scheduleJob</value>
				<value>purgeJob</value>
				<value>fiscalYearMakerJob</value>
				<value>purgeReportsAndStagingJob</value>
				<value>autoDisapproveJob</value>
		                <value>dailyEmailJob</value>
