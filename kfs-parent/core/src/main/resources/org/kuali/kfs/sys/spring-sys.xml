       <property name="jaxWsService" value="true"/>
       <property name="priority" value="3" />
       <property name="retryAttempts" value="0" />
       <!-- TODO: make config property for bus security -->
       <property name="busSecurity" value="false"/>
    </bean>

    <bean id="kfsJaxWsRestService" class="org.kuali.rice.ksb.messaging.RESTServiceDefinition" abstract="true">
       <property name="serviceNameSpaceURI" value="KFS"/>
       <property name="priority" value="3" />
       <property name="retryAttempts" value="0" />
       <property name="busSecurity" value="false"/>
    </bean>

    
